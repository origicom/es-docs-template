{{!include(favicon.png)}}
{{!include(logo.svg)}}
{{!include(search-stopwords.json)}}
<!DOCTYPE html>
<!--[if IE]><![endif]-->
<html>
  {{>partials/head}}
  <body data-spy="scroll" data-target="#affix" onload="checkCookies()">
    <div class="cookie-notice" id="cookies">
        <div class="cookie-notice__banner">
            <div class="container">
                <h2>Cookies preferences</h2>
                <p>We’d like to use analytics cookies. These send information about how our site is used to services called Google Analytics and LinkedIn. We’ll use this information to improve our site. Let us know if this is okay. We’ll use a cookie to save your choice. You can <a href="/privacy/#tracking">read more about our cookies</a> before you choose.</p>
                <button class="btn btn--primary" onclick="allowCookies()">I’m okay with analytics cookies</button>
                <button class="btn btn--primary" onclick="revokeCookieConsent()">Do not use analytics cookies</button>
            </div>
        </div>
    </div>
    <div id="wrapper">
      <header class="site-banner" role="banner">
        {{^_disableNavbar}}
        {{>partials/navbar}}
        {{/_disableNavbar}}
        {{^_disableBreadcrumb}}
        {{>partials/breadcrumb}}
        {{/_disableBreadcrumb}}
      </header>
      {{#_enableSearch}}
      <div class="container body-content">
        {{>partials/searchResults}}
      </div>
      {{/_enableSearch}}
      <div role="main" class="container body-content hide-when-search">
        <!--  TODO: Currently override (git history) shouldn't need to.-->
        {{>partials/toc}}
        <div class="article row grid-right">
          {{#_disableAffix}}
          <div class="col-md-12">
          {{/_disableAffix}}
          {{^_disableAffix}}
          <div class="col-md-10">
          {{/_disableAffix}}
            <article class="content wrap" id="_content" data-uid="{{uid}}">
                {{!body}}
            </article>
          </div>
          {{^_disableAffix}}
          {{>partials/affix}}
          {{/_disableAffix}}
        </div>
      </div>
      {{^_disableFooter}}
      {{>partials/footer}}
      {{/_disableFooter}}
    </div>
    {{>partials/scripts}}
  </body>
</html>
